{
    "name": "riskio/specification",
    "description": "PHP implementation of the specification pattern",
    "homepage": "https://github.com/RiskioFr/Specification",
    "require": {
        "php": "^7"
    },
    "require-dev": {
        "phpunit/phpunit": "^5"
    },
    "autoload": {
        "psr-4": {
            "Riskio\\Specification\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Riskio\\Specification\\": "tests/"
        },
        "files": ["vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"]
    }
}
